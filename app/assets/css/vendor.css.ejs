/*Bootstrap font paths fix*/
@font-face {
  font-family: 'Glyphicons Halflings';

  src: url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.eot') %>');
  src: url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.eot') %>?#iefix') format('embedded-opentype'), url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.woff2') %>') format('woff2'), url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.woff') %>') format('woff'), url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.ttf') %>') format('truetype'), url('<%- assetPath('bootstrap/fonts/glyphicons-halflings-regular.svg') %>#glyphicons_halflingsregular') format('svg');
}
